{
	"name":"tncrazvan/catpaw-template",
	"type": "project",
	"description": "A php CatPaw template",
	"keywords": ["php","cli","server","http","websocket","catpaw","template"],
	"homepage": "https://github.com/tncrazvan/catpaw-template",
	"license": "Apache-2.0",
	"scripts":{
		"dev": "sudo ./scripts/dev.sh",
		"mkcert": "sudo php ./scripts/mkcert.php",
		"start": "sudo ./scripts/start.sh",
		"enable-scripts":"sudo chmod +x ./scripts/*",
		"sanitize-scripts": "sudo ./scripts/sanitizeln-default.sh",
		"ccli":"sudo php ./scripts/ccli/main.php"
	},
	"authors": [
		{
			"name": "Razvan Tanase",
			"email": "tangent.jotey@gmail.com",
			"homepage": "https://github.com/tncrazvan",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"scripts\\":"scripts/",
			"":"src/"
		}
	},
	"require": {
		"php": ">=8.0",
		"tncrazvan/catpaw":">=2.3.5",
		"react/react": "^1.1"
	},
	"config": {
		"preferred-install": "dist",
		"process-timeout":0
	}
}
