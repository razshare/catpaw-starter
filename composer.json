{
  "name": "catpaw/starter",
  "type": "project",
  "description": "A php CatPaw starter template",
  "homepage": "https://github.com/tncrazvan/catpaw-starter",
  "license": "MIT",
  "authors": [
    {
      "name": "Razvan Tanase",
      "email": "tangent.jotey@gmail.com",
      "homepage": "https://github.com/tncrazvan",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/lib/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^8.2",
    "ext-mbstring": "*",
    "ext-pcntl": "*",
    "ext-openssl": "*",
    "ext-zlib": "*",
    "catpaw/catpaw": "^4.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "phpunit/phpunit": "^9",
    "phpstan/phpstan": "^1.10"
  },
  "config": {
    "preferred-install": "dist",
    "process-timeout": 0
  }
}
