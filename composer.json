{
	"name":"razshare/catpaw-starter",
	"type": "project",
	"description": "A php CatPaw starter template",
	"keywords": ["php","cli","server","http","websocket","catpaw","starter","template"],
	"homepage": "https://github.com/tncrazvan/catpaw-starter",
	"license": "GPL-2.0-only",
	"scripts":{
		"mkcert": "php ./vendor/razshare/catpaw/scripts/mkcert.php",
		"enable-scripts":"sudo chmod +x ./vendor/razshare/catpaw/scripts/*",
		"ccli":"sudo php ./vendor/razshare/catpaw/scripts/ccli/main.php",
		"load":"composer dump-autoload -o",
		"start": "php vendor/razshare/catpaw/scripts/start.php src/main.php",
		"dev": "php vendor/razshare/catpaw/scripts/start.php src/main.php dev 100 && @composer run dev"
	},
	"authors": [
		{
			"name": "Razvan Tanase",
			"email": "tangent.jotey@gmail.com",
			"homepage": "https://github.com/tncrazvan",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"App\\":"src/"
		}
	},
	"require": {
		"php": ">=8.0",
		"razshare/catpaw":"^2.0.0",
		"amphp/amp": "^2.6",
		"amphp/file": "^2.0"
	},
	"config": {
		"preferred-install": "dist",
		"process-timeout":0
	}
}
