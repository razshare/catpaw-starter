{
	"name":"razshare/catpaw-starter",
	"type": "project",
	"description": "A php CatPaw starter template",
	"keywords": ["php","cli","server","http","websocket","catpaw","starter","template"],
	"homepage": "https://github.com/tncrazvan/catpaw-starter",
	"license": "GPL-2.0-only",
	"scripts":{
		"dev": "sudo php ./scripts/start.php dev 100",
		"mkcert": "sudo php ./scripts/mkcert.php",
		"start": "sudo php ./scripts/start.php",
		"enable-scripts":"sudo chmod +x ./scripts/*",
		"ccli":"sudo php ./scripts/ccli/main.php",
		"load":"composer dump-autoload -o"
	},
	"authors": [
		{
			"name": "Razvan Tanase",
			"email": "tangent.jotey@gmail.com",
			"homepage": "https://github.com/tncrazvan",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"scripts\\":"scripts/",
			"app\\":"src/"
		}
	},
	"require": {
		"php": ">=8.0",
		"razshare/catpaw":">=1.2.0",
		"react/react": ">=1.1"
	},
	"config": {
		"preferred-install": "dist",
		"process-timeout":0
	}
}
